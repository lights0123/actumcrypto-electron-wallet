{
	"name": "actumcrypto-wallet",
	"version": "0.0.0",
	"author": "lights0123 <me@lights0123.com>",
	"description": "A wallet for the actumcrypto cryptocurrency",
	"license": null,
	"main": "./dist/main/build.js",
	"scripts": {
		"start": "electron ."
	},
	"build": {
		"productName": "actumcrypto-wallet",
		"appId": "org.simulatedgreg.electron-vue",
		"directories": {
			"output": "dist"
		},
		"files": [
			"dist/**/*"
		],
		"dmg": {
			"contents": [
				{
					"x": 410,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				},
				{
					"x": 130,
					"y": 150,
					"type": "file"
				}
			]
		},
		"mac": {
			"icon": "static/icons/icon.icns"
		},
		"win": {
			"icon": "static/icons/icon.ico"
		},
		"linux": {
			"icon": "static/icons"
		}
	},
